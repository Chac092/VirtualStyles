<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_rellenarPrendas">

    <ImageView
        android:id="@+id/FotoPrinc"
        android:layout_width="291dp"
        android:layout_height="281dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/holo_red_dark" />
    <Button
        android:id="@+id/BTNBorrar"
        android:layout_width="101dp"
        android:layout_height="45dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="160dp"
        android:text="@string/delete"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <ImageView
        android:id="@+id/BTNasignarGorro"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="68dp"
        app:layout_constraintBottom_toTopOf="@+id/BTNBorrar"
        app:layout_constraintEnd_toStartOf="@+id/BTNasignarCamiseta"
        app:srcCompat="@drawable/gorra" />
    <ImageView
        android:id="@+id/BTNasignarCamiseta"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="68dp"
        app:layout_constraintBottom_toTopOf="@+id/BTNBorrar"
        app:layout_constraintEnd_toStartOf="@+id/BTNasignarPantalon"
        app:srcCompat="@drawable/camisa" />
    <ImageView
        android:id="@+id/BTNasignarPantalon"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="68dp"
        app:layout_constraintBottom_toTopOf="@+id/BTNBorrar"
        app:layout_constraintEnd_toStartOf="@+id/BTNasignarZapatillas"
        app:srcCompat="@drawable/pantalon" />
    <ImageView
        android:id="@+id/BTNasignarZapatillas"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="68dp"
        app:layout_constraintBottom_toTopOf="@+id/BTNBorrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/zapatos" />
</androidx.constraintlayout.widget.ConstraintLayout>
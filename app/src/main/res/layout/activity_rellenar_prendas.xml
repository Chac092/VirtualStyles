<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_rellenarPrendas">

    <ImageView
        android:id="@+id/FotoPrinc"
        android:layout_width="291dp"
        android:layout_height="281dp"
        android:layout_marginTop="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:color/holo_red_dark" />

    <Button
        android:id="@+id/BTNBorrar"
        android:layout_width="101dp"
        android:layout_height="45dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="160dp"
        android:text="Rechazar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/BTNBorrar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FotoPrinc"
        app:layout_constraintVertical_bias="0.958"
        app:srcCompat="@drawable/gorra" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_marginStart="84dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/BTNBorrar"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toBottomOf="@+id/FotoPrinc"
        app:layout_constraintVertical_bias="0.958"
        app:srcCompat="@drawable/camisa" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_marginStart="84dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/BTNBorrar"
        app:layout_constraintStart_toStartOf="@+id/imageView20"
        app:layout_constraintTop_toBottomOf="@+id/FotoPrinc"
        app:layout_constraintVertical_bias="0.958"
        app:srcCompat="@drawable/pantalon" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="78dp"
        android:layout_height="73dp"
        android:layout_marginStart="84dp"
        android:layout_marginBottom="44dp"
        app:layout_constraintBottom_toTopOf="@+id/BTNBorrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView18"
        app:layout_constraintTop_toBottomOf="@+id/FotoPrinc"
        app:layout_constraintVertical_bias="0.958"
        app:srcCompat="@drawable/zapatos" />

</androidx.constraintlayout.widget.ConstraintLayout>